<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="moneymanager.app.com">

    <application
        android:name=".factory.MainApplication_"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".domains.main.MainActivity_"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name=".domains.home.add_item.AddItemActivity_"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".domains.home.detail.ItemDetailActivity_"
            android:screenOrientation="portrait"/>
    </application>

</manifest>